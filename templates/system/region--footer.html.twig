{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
<footer{{ attributes.addClass('footer') }} role="contentinfo">
  <div class="container">
    &copy; <a href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home">{{- site_name|default('Home') -}}</a>
    {% if content %}
      {{ content }}
    {% endif %}
    <img class="footer-logo" height="50" src="{{ tieto_base.images ~ '/logo_footer.svg' }}" alt="{{ site_name|default('Home') }}">
  </div>
</footer>
