{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
<footer{{ attributes.addClass('footer', 'container') }} role="contentinfo">
  <div class="footer__content">
    <a class="footer__item" href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home">&copy; {{ site_name|default('Home') }}</a>
    {% if content %}
      {{ content }}
    {% endif %}
    <a class="footer__item footer__item--logo" href="https://www.tieto.com" title="{{ 'Go to Tieto.com'|t }}" target="_blank" rel="noopener">
      <img width="150" src="{{ tieto_base.images ~ '/logo_footer.svg' }}" alt="Tieto Logo">
    </a>
  </div>
</footer>
