{
	"styles": {
		"watch": "./assets/scss/**/*.{sass,scss}",
		"entry": "./assets/scss/global.scss",
		"dest": "./dist/css"
	},
	"scripts": {
		"watch": "./assets/scripts/**/*.js",
		"entry": "./assets/scripts/**/*.js",
		"dest": "./dist/js"
	},
	"fonts": {
		"watch": "./assets/fonts/**/*.{ttf,otf}",
		"entry": "./assets/fonts/**/*.{ttf,otf}",
		"dest": "./dist/fonts"
	},
	"browserSync": {
		"notify": false
	}
}
